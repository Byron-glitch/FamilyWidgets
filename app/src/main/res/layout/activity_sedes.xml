<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Activity_sedes" android:id="@+id/btn">

    <TextureView
            android:layout_width="385dp"
            android:layout_height="196dp" android:id="@+id/textureView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="460dp"/>

    <TextView
            android:text="Sedes de la Universidad Nacional"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView2"
            android:layout_marginBottom="32dp"
            app:layout_constraintBottom_toTopOf="@+id/textureView" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="101dp" android:layout_marginEnd="101dp"
            app:layout_constraintEnd_toEndOf="parent" android:textColor="#D10606" android:textSize="20dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>
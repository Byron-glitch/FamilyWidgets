<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity" android:id="@+id/btn_onResume">


    <ImageView
            android:layout_width="89dp"
            android:layout_height="78dp" app:srcCompat="@android:mipmap/sym_def_app_icon"
            android:id="@+id/imageView"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:onClick="do_onclickImage"
            app:layout_constraintVertical_bias="0.082"/>
    <Button
            android:text="Imagen 2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btn_image_2"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="148dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="88dp"/>
    <Button
            android:text="-"
            android:layout_width="50dp"
            android:layout_height="50dp" android:id="@+id/btn_decrease_size"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="72dp" android:layout_marginStart="80dp"/>
    <Button
            android:text="+"
            android:layout_width="50dp"
            android:layout_height="50dp" android:id="@+id/btn_increase_size"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="76dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="72dp"/>
    <Button
            android:text="imagen 1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btn_image_1"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="148dp" android:layout_marginStart="88dp"/>
    <WebView
            android:id="@+id/webView"
            android:layout_width="398dp"
            android:layout_height="199dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView"
            app:layout_constraintVertical_bias="0.312" app:layout_constraintHorizontal_bias="0.461"
            />
    <Button
            android:text=" go forward"
            android:layout_width="120dp"
            android:layout_height="60dp" android:id="@+id/btn_goforward"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="120dp" app:layout_constraintHorizontal_bias="0.776"
            android:backgroundTint="#2CCD10"/>
    <Button
            android:text="go back"
            android:layout_width="120dp"
            android:layout_height="60dp" android:id="@+id/btn_goback"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="120dp" app:layout_constraintHorizontal_bias="0.23"
            android:backgroundTint="#E80A51"/>
    <Button
            android:text="ONRESUME"
            android:layout_width="120dp"
            android:layout_height="60dp" android:id="@+id/btn_resume"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="180dp" app:layout_constraintHorizontal_bias="0.776"
            android:backgroundTint="#009688"/>
    <Button
            android:text="onPause"
            android:layout_width="120dp"
            android:layout_height="60dp" android:id="@+id/btn_pause"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.23" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="180dp"
            android:backgroundTint="#FF9800"/>
    <EditText
            android:layout_width="250dp"
            android:layout_height="49dp"
            android:inputType="textPersonName"
            android:text=""
            android:hint="https://www.example.com"
            android:ems="10"
            android:id="@+id/txt_url"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/webView" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.496" app:layout_constraintVertical_bias="0.681"/>
    <TextView
            android:text="IMAGE VIEW"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="16dp"
            app:layout_constraintHorizontal_bias="0.498"
            android:textSize="20dp"/>
    <TextView
            android:text="WEB VIEW"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView2"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="204dp"
            app:layout_constraintHorizontal_bias="0.498"
            android:textSize="20dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>